{
  "manifest_version": 3,
  "name": "TwitterDeAddictMe",
  "description": "Puts a text that reduces your time on twitter",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*", "*://x.com/*"],
      "js": ["content.js"]
    }
  ],
  "version": "1.0"
}